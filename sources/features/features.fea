# ==================================================# Basic OpenType Features for Bungee# ==================================================# CLASSES@ordn_digits = [zero one two three four five six seven eight nine];@figures = [zero one two three four five six seven eight nine];###@salt_A = [A.salt Agrave.salt Aacute.salt Acircumflex.salt Atilde.salt Adieresis.salt Aring.salt Amacron.salt Abreve.salt Aogonek.salt AE.salt];@salt_A_off = [A Agrave Aacute Acircumflex Atilde Adieresis Aring Amacron Abreve Aogonek AE];@salt_a_off = [a agrave aacute acircumflex atilde adieresis aring amacron abreve aogonek ae];@salt_E = [E.salt Egrave.salt Eacute.salt Ecircumflex.salt Edieresis.salt Emacron.salt Ebreve.salt Edotaccent.salt Eogonek.salt Ecaron.salt ];@salt_E_off = [E Egrave Eacute Ecircumflex Edieresis Emacron Ebreve Edotaccent Eogonek Ecaron ];@salt_e_off = [e egrave eacute ecircumflex edieresis emacron ebreve edotaccent eogonek ecaron ];@salt_I = [I.salt Igrave.salt Iacute.salt Icircumflex.salt Idieresis.salt Itilde.salt Imacron.salt Ibreve.salt Iogonek.salt Idotaccent.salt];@salt_I_off = [I Igrave Iacute Icircumflex Idieresis Itilde Imacron Ibreve Iogonek Idotaccent];@salt_i = [I.salt Igrave.salt Iacute.salt Icircumflex.salt Idieresis.salt Itilde.salt Imacron.salt Ibreve.salt Iogonek.salt];@salt_i_off = [i igrave iacute icircumflex idieresis itilde imacron ibreve iogonek];@salt_L = [L.salt Lacute.salt Lcommaaccent.salt Lcaron.salt Ldot.salt Lslash.salt];@salt_L_off = [L Lacute Lcommaaccent Lcaron Ldot Lslash];@salt_l_off = [l lacute lcommaaccent lcaron ldot lslash];@salt_M = [M.salt];@salt_M_off = [M];@salt_m_off = [m];@salt_N = [N.salt Ntilde.salt Nacute.salt Ncommaaccent.salt Ncaron.salt];@salt_N_off = [N Ntilde Nacute Ncommaaccent Ncaron];@salt_n_off = [n ntilde nacute ncommaaccent ncaron];@salt_W = [W.salt Wcircumflex.salt Wgrave.salt Wacute.salt Wdieresis.salt];@salt_W_off = [W Wcircumflex Wgrave Wacute Wdieresis];@salt_w_off = [w wcircumflex wgrave wacute wdieresis];@salt_Y = [Y.salt Yacute.salt Ycircumflex.salt Ydieresis.salt Ygrave.salt];@salt_Y_off = [Y Yacute Ycircumflex Ydieresis Ygrave];@salt_y_off = [y yacute ycircumflex ydieresis ygrave];@salt_X = [X.salt];@salt_X_off = [X];@salt_x_off = [x];####@salt_quote = [quoteleft.salt quoteright.salt];@salt_quote_off = [quoteleft quoteright];####@salt_ampersand = [ampersand.salt];@salt_ampersand_off = [ampersand];############@arrows = [arrowleft arrowup arrowright arrowdown arrowupleft arrowupright arrowdownright arrowdownleft];@indexes = [uni261A uni261B indexup indexdown indexupleft indexupright indexdownright indexdownleft];@indexes_salt = [uni261A.salt uni261B.salt indexup.salt indexdown.salt indexupleft.salt indexupright.salt indexdownright.salt indexdownleft.salt];@indexes_outline = [uni261C uni261D uni261E uni261F indexupleft.outline indexupright.outline indexdownright.outline indexdownleft.outline];@indexes_salt_outline = [uni261C.salt uni261D.salt uni261E.salt uni261F.salt indexdownright.salt_outline indexupleft.salt_outline indexdownleft.salt_outline indexupright.salt_outline];####@arrows_bent = [uni21B0 uni21B1 uni21B2 uni21B3 uni21AB uni21AA];@ornm = [@arrows @indexes @indexes_salt @indexes_outline @indexes_salt_outline @arrows_bent];@downer_off = [I Igrave Iacute Icircumflex Idieresis Itilde Imacron Ibreve Iogonek Idotaccent J Jcircumflex L Lacute Lcommaaccent Lcaron M W Wcircumflex Wgrave Wacute Wdieresis I.salt Igrave.salt Iacute.salt Icircumflex.salt Idieresis.salt Itilde.salt Imacron.salt Ibreve.salt Iogonek.salt Idotaccent.salt L.salt Lacute.salt Lcommaaccent.salt Lcaron.salt Ldot.salt Lslash.salt M.salt W.salt Wcircumflex.salt Wgrave.salt Wacute.salt Wdieresis.salt i igrave iacute icircumflex idieresis itilde imacron ibreve iogonek j jcircumflex l lacute lcommaaccent m w wcircumflex wgrave wacute wdieresis dotlessi];@downer = [.notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef .notdef];languagesystem DFLT dflt;languagesystem latn dflt;languagesystem latn ROM;languagesystem latn CAT;feature locl { # Localized Forms    script latn; # Latin        language ROM; # Romanian            lookup lookup_ROM {                 sub [Scedilla scedilla] by [Scommaaccent scommaaccent];            } lookup_ROM;        language CAT; # Catalan            sub L' periodcentered L by Ldot;            sub l' periodcentered l by ldot;} locl;feature ordn { # Ordinals    sub @ordn_digits a' by ordfeminine;    sub @ordn_digits o' by ordmasculine;} ordn;feature ss02 { # Round forms    featureNames {        name "Round forms";        name 1 "Round forms";	};    lookup salt_A {        sub @salt_A_off by @salt_A;        sub @salt_a_off by @salt_A;    } salt_A;    lookup salt_M {        sub @salt_M_off by @salt_M;        sub @salt_m_off by @salt_M;    } salt_M;    lookup salt_N {        sub @salt_N_off by @salt_N;        sub @salt_n_off by @salt_N;    } salt_N;    lookup salt_W {        sub @salt_W_off by @salt_W;        sub @salt_w_off by @salt_W;    } salt_W;    lookup salt_X {        sub @salt_X_off by @salt_X;        sub @salt_x_off by @salt_X;    } salt_X;    lookup salt_Y {        sub @salt_Y_off by @salt_Y;        sub @salt_y_off by @salt_Y;    } salt_Y;           } ss02;feature ss03 { # Round E    featureNames {        name "Round E";        name 1 "Round E";	};    lookup salt_E {        sub @salt_E_off by @salt_E;        sub @salt_e_off by @salt_E;    } salt_E;} ss03;feature ss04 { # Serifless I    featureNames {        name "Sans-serif I";        name 1 "Sans-serif I";	};    lookup salt_I {        sub @salt_I_off by @salt_I;        sub @salt_i_off by @salt_i;    } salt_I;} ss04;feature ss05 { # Round L    featureNames {        name "Sans-serif L";        name 1 "Sans-serif L";	};    lookup salt_L {        sub @salt_L_off by @salt_L;        sub @salt_l_off by @salt_L;    } salt_L;} ss05;feature ss06 { # Alternate Ampersand    featureNames {        name "Alternate ampersand";        name 1 "Alternate ampersand";	};    lookup salt_ampersand {        sub @salt_ampersand_off by @salt_ampersand;    } salt_ampersand;} ss06;feature ss07 { # Small Apostrophe    featureNames {        name "Small quotes";        name 1 "Small quotes";	};    lookup salt_quote {        sub @salt_quote_off by @salt_quote;    } salt_quote;} ss07;feature salt { # Stylistic alternates    lookup salt_A;                lookup salt_M;                lookup salt_N;                lookup salt_W;                lookup salt_X;                lookup salt_Y;				    ##    lookup salt_E;    lookup salt_I;	lookup salt_L;    ##	##    lookup salt_ampersand;    lookup salt_quote;    ##    lookup indexes {        sub @arrows by @indexes;     } indexes;    lookup salt_indexes {        sub @arrows by @indexes_salt;        sub @indexes by @indexes_salt;     } salt_indexes;    lookup salt_indexes_outline {        sub @indexes by @indexes_outline;        sub @indexes_salt by @indexes_salt_outline;     } salt_indexes_outline;} salt;feature ornm { # Ornaments    sub bullet from @ornm;} ornm;